{
  "name": "yt-dlp-interface-electron",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export && tsc --project tsconfig.electron.json && npm run copy-yt-dlp",
    "copy-yt-dlp": "copyfiles -u 1 ./bin/yt-dlp.exe ./dist/bin/",
    "start": "next start",
    "electron": "tsc --project tsconfig.electron.json && electron dist/main.js",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "next": "15.1.7",
    "node-fetch": "^2.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "electron": "^34.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "wait-on": "^8.0.2"
  }
}
